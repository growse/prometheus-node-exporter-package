{
  "extends": [
    "config:base",
    ":disableRateLimiting",
    ":rebaseStalePrs",
    ":disableDependencyDashboard"
  ],
  "assignees": ["growse"],
  "regexManagers": [
    {
      "fileMatch": ["^GNUmakefile$"],
      "matchStrings": ["NODE_EXPORTER_VERSION := v(?<currentValue>.*?)"],
      "depNameTemplate": "prometheus/node_exporter",
      "datasourceTemplate": "github-releases"
    }
  ]
}
